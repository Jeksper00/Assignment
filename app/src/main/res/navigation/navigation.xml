        tools:layout="@layout/admin_fragment_manageuser" >
        <action
            android:id="@+id/action_adminManageUserFragment_to_adminLoginActivity"
            app:destination="@id/adminLoginActivity" />
    </fragment>
    <fragment
        android:id="@+id/userActivityFragment"
        android:name="com.example.assignment.UserFragment.UserActivityFragment"
        android:label="user_fragment_activity"
        tools:layout="@layout/user_fragment_activity" >
        <action
            android:id="@+id/action_userActivityFragment_to_userLoginActivity"
            app:destination="@id/userLoginActivity"
            app:popUpTo="@id/userLoginActivity"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/userHomeFragment"
        android:name="com.example.assignment.UserFragment.UserHomeFragment"
        android:label="user_fragment_home"
        tools:layout="@layout/user_fragment_home" >
