            android:id="@+id/action_userHomeFragment_to_userLoginActivity"
            app:destination="@id/userLoginActivity"
            app:popUpTo="@id/userLoginActivity"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/userNotificationFragment"
        android:name="com.example.assignment.UserFragment.UserNotificationFragment"
        android:label="user_fragment_notification"
